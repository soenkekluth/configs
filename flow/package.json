{
  "name": "scrollemitter",
  "version": "1.1.3",
  "description": "enhanced scroll events like scroll: start, progress, stop, min, max",
  "main": "scrollemitter.js",
  "browser": "scrollemitter.min.js",
  "scripts": {
    "build": "cross-env NODE_ENV=production run-s clear bundle build:js",
    "build:js": "parcel build src/index.js --public-url ./ -d dist",
    "build:js:d": "parcel build src/index.js --public-url ./ --no-minify -d dist",
    "build:lib": "cross-env NODE_ENV=production rimraf lib && babel src -d lib && run-s build:es5",
    "build:es5": "cross-env NODE_ENV=production rimraf lib/es5 && mkdirp lib/es5 && BABEL_ENV=es5 babel src -d lib/es5",
    "bundle": "microbundle build -i lib/index.js -o lib/build",
    "prettier": "prettier --write 'src/**/*.js' > /dev/null",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build-": "size docs",
    "clear": "rimraf lib dist && mkdirp lib",
    "examples": "serve -o -s -C -c 1 examples/",
    "bump": "standard-version",
    "semantic": "semantic-release",
    "docs": "documentation readme src/scrollemitter.js --section API -q",
    "lint:fix": "eslint src test --fix",
    "lint": "eslint src test",
    "flow-typed": "flow-typed install --ignoreDeps peer dev",
    "prerelease": "run-s bump build && git commit -a -m 'version: bump version' && git push --follow-tags origin master",
    "release": "run-s copy && cd lib && npm publish",
    "copy": "cp -rf ./package.json lib && cp -rf ./README.md lib && cp -rf ./dist/index.js lib/scrollemitter.min.js",
    "size": "echo \"Gzipped Size: $(strip-json-comments --no-whitespace dist/index.js | gzip-size)\"",
    "watch": "parcel --public-url ./ src/index.html",
    "postrelease-": "bs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/soenkekluth/scrollemitter.git"
  },
  "keywords": [
    "scroll",
    "scroll direction",
    "scroll speed",
    "scrolling",
    "scroll features",
    "browser",
    "enhanced",
    "onscroll",
    "animationframe",
    "animation",
    "enhanced scroll",
    "scrollevents",
    "scroll event",
    "scroll behavior",
    "fast",
    "jankfree",
    "jank",
    "jank-free",
    "perfomance",
    "scroll perfomance",
    "perfomant"
  ],
  "author": "SÃ¶nke Kluth <soenke.kluth@gmail.com> (http://soenkekluth.com/)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/soenkekluth/scrollemitter/issues"
  },
  "homepage": "https://github.com/soenkekluth/scrollemitter#readme",
  "dependencies": {
    "@babel/runtime": "^7.0.0-beta.40",
    "micromitter": "1.0.4",
    "raf": "^3.4.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.40",
    "@babel/core": "^7.0.0-beta.40",
    "@babel/plugin-proposal-class-properties": "^7.0.0-beta.40",
    "@babel/plugin-proposal-export-default-from": "^7.0.0-beta.40",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0-beta.40",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0-beta.40",
    "@babel/plugin-transform-modules-commonjs": "^7.0.0-beta.40",
    "@babel/plugin-transform-runtime": "^7.0.0-beta.40",
    "@babel/preset-env": "^7.0.0-beta.40",
    "@babel/preset-flow": "^7.0.0-beta.40",
    "babel-core": "^7.0.0-0",
    "babel-eslint": "^8.2.1",
    "babel-jest": "^22.2.2",
    "babel-plugin-resolver": "^1.1.0",
    "branchsite": "^4.0.3",
    "cross-env": "^5.1.3",
    "documentation": "^5.4.0",
    "eslint": "^4.18.0",
    "eslint-plugin-flowtype": "^2.44.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jest": "^21.12.2",
    "eslint-plugin-node": "^6.0.0",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-promise": "^3.6.0",
    "filesize": "^3.6.0",
    "flow-bin": "^0.66.0",
    "glob": "^7.1.2",
    "gzip-size": "^4.1.0",
    "gzip-size-cli": "^2.1.0",
    "husky": "^0.14.3",
    "jest": "^22.3.0",
    "jest-junit": "^3.6.0",
    "microbundle": "^0.4.3",
    "mkdirp": "^0.5.1",
    "np": "^2.19.0",
    "npm-run-all": "^4.1.2",
    "parcel-bundler": "^1.6.1",
    "prettier": "^1.10.2",
    "prettier-stylelint": "^0.4.2",
    "rimraf": "^2.6.2",
    "semantic-release": "^12.4.1",
    "serve": "^6.4.10",
    "standard-version": "^4.3.0",
    "strip-json-comments-cli": "^1.0.1"
  }
}
